[sub_resource type="GDScript" id="GDScript_2orkt"]
script/source = "extends TextureProgressBar

@export var player_group: StringName = &\"player\"
@export var smooth_speed: float = 10.0 # 0 = без плавности

var player: Node = null
var display_value: float = 0.0

func _ready() -> void:
	_find_player()

func _process(delta: float) -> void:
	# если игрока нет — обнуляем бар
	if player == null or not is_instance_valid(player):
		player = null
		display_value = 0.0
		value = 0.0
		_find_player()
		return

	var max_hp := _read_max_hp(player)
	var hp := _read_hp(player)

	if max_hp <= 0:
		max_value = 1.0
		value = 0.0
		display_value = 0.0
		return

	max_value = float(max_hp)

	var target := float(hp)

	# если игрок умер (hp <= 0) — сразу пусто
	if hp <= 0:
		display_value = 0.0
		value = 0.0
		return

	if smooth_speed <= 0.0:
		value = target
	else:
		display_value = lerp(display_value, target, smooth_speed * delta)
		value = display_value

func _find_player() -> void:
	var nodes := get_tree().get_nodes_in_group(player_group)
	for n in nodes:
		if _has_hp(n):
			player = n
			display_value = float(_read_hp(player))
			max_value = float(_read_max_hp(player))
			value = display_value
			return

func _has_hp(n: Node) -> bool:
	return (n.has_method(\"get_hp\") and n.has_method(\"get_max_hp\")) or ((\"hp\" in n) and (\"max_hp\" in n))

func _read_hp(n: Node) -> int:
	if n.has_method(\"get_hp\"):
		return int(n.call(\"get_hp\"))
	return int(n.hp)

func _read_max_hp(n: Node) -> int:
	if n.has_method(\"get_max_hp\"):
		return int(n.call(\"get_max_hp\"))
	return int(n.max_hp)
"

